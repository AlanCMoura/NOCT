{
  "expo": {
    "name": "Container View",
    "slug": "containerview",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UISystemSoundIDs": []
      }
    },
    "android": {
      "config": {
        "allowBackup": false
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.containerview"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/NOCT.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow this app to access your camera",
          "microphonePermission": "Allow this app to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_intro.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}